{
  "name": "canton-wallet-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit",
    "test:ui": "vitest --ui --config vitest.config.unit.js",
    "test:coverage": "vitest --coverage --config vitest.config.unit.js",
    "test:unit": "vitest run --config vitest.config.unit.js --reporter=verbose",
    "test:unit:watch": "vitest --config vitest.config.unit.js",
    "test:integration": "vitest run --config vitest.config.integration.js --reporter=verbose",
    "test:integration:watch": "vitest --config vitest.config.integration.js",
    "test:integration:real": "npm run localnet:check && RUN_REAL_INTEGRATION_TESTS=true npm run test:integration",
    "test:integration:real:force": "RUN_REAL_INTEGRATION_TESTS=true npm run test:integration",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:all:real": "npm run test:unit && npm run test:integration:real",
    "localnet:check": "node scripts/check-localnet.js",
    "localnet:wait": "node scripts/check-localnet.js --wait",
    "localnet:status": "node scripts/check-localnet.js --verbose",
    "verify": "npm run test:unit && npm run build",
    "verify:full": "npm run test:all:real && npm run build",
    "postinstall": "node scripts/patch-canton-sdk.js",
    "server:dev": "nodemon --watch server --ext js --exec node server/index.js",
    "server:start": "node server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@canton-network/core-ledger-client": "^0.5.0",
    "@canton-network/wallet-sdk": "^0.5.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@vitejs/plugin-react": "^5.0.2",
    "buffer": "^6.0.3",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.0",
    "jose": "5.2.4",
    "pino": "^9.9.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^13.0.0",
    "vite": "^7.1.5",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.0",
    "@vitest/ui": "^3.2.4",
    "chalk": "^5.3.0",
    "jsdom": "^27.0.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
